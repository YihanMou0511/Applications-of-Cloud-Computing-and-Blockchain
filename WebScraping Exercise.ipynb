   "source": [
    "### Q3.B.) Webcrawl the links from A and use RegEx to get all the emails and phone numbers of ladder faculty profiles"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 124,
   "id": "4fa6625b",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_email(URL):\n",
    "    soup=lovely_soup(URL)\n",
    "    text=str(soup)\n",
    "    pattern=re.compile(r'\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b')\n",
    "    matches=pattern.finditer(text)\n",
    "    matches=[match for match in matches]\n",
    "    return(matches[0][0])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 142,
   "id": "92b3461d",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_phone(URL):\n",
    "    soup=lovely_soup(URL)\n",
    "    text=str(soup)\n",
    "    pattern=re.compile(r'Phone:\\<\\/h4\\> \\(?\\d{3}\\)?[-.\\s]?\\d{3}[-.\\s]?\\d{4}')\n",
    "    matches=pattern.finditer(text)\n",
    "    matches=[match for match in matches]\n",
    "    if matches == []:\n",
    "        return(None)\n",
    "    else:\n",
    "        phone=matches[0][0]\n",
    "        pattern2=re.compile(r'\\(?\\d{3}\\)?[-.\\s]?\\d{3}[-.\\s]?\\d{4}')\n",
    "        matches2=pattern2.finditer(phone)\n",
    "        matches2=[match for match in matches2]\n",
    "        phone2=matches2[0][0]\n",
    "        return(phone2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 143,
   "id": "7d65ca81",
   "metadata": {},
   "outputs": [],
   "source": [
    "phone_list=[]\n",
    "email_list=[]\n",
    "for url in profiles:\n",
    "    phone_list.append(get_phone(url))\n",
    "    email_list.append(get_email(url))"
   ]
  },
